<script>
    window.addEventListener("load", function(event) {
        setTimeout(function() {
            var rutInput = document.querySelector("input[name='Contact.CustomFields.c.rut']");
            rutInput.addEventListener("change", function(e) {
                e.target.value = e.target.value.replace(/-/g, "");
                if (e.target.value.length == 8) {
                    e.target.value = "0" + e.target.value;
                }
                if ((e.target.value.length == 9 && e.target.value.indexOf("-") < 0)) {
                    e.target.value = e.target.value.slice(0, 8) + "-" + e.target.value.slice(8);
                }
            })
        }, 200)
        document.getElementById("rn_QuestionSubmit").addEventListener('submit', function(evt){
            evt.preventDefault();
            // console.log("Estou aqui, evc nao vai enviar nao");
        })
        document.querySelector("#rn_ProductCategoryInput_v2_10_Category_Button").disabled = true;
        function disabledInputDetalhe(){
            var txt = document.querySelector("#rn_ProductCategoryInput_v2_9_Button_Visible_Text").innerText;
            // console.log(txt);
            if(txt != "Seleccionar"){
                document.querySelector("#rn_ProductCategoryInput_v2_10_Category_Button").disabled = false;
            }else{
                document.querySelector("#rn_ProductCategoryInput_v2_10_Category_Button").disabled = true;
            } 
        }
        var items = document.querySelector("#rn_ProductCategoryInput_v2_9_Tree").querySelectorAll(".ygtvcell");
        for (let index = 0; index < items.length; index++) {
            const element = items[index];
            element.addEventListener("click",function() {
                setTimeout(function(){
                    disabledInputDetalhe() 
                }, 100);
            });
            element.addEventListener("keydown",function(e) {
                // console.log(e.code);
                if(e.code == "Enter"){
                    setTimeout(function(){
                        disabledInputDetalhe() 
                    }, 100);
                }
            });
        }
        // console.log("Todos os recursos terminaram o carregamento!");
  });
</script>

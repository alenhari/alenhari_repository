<script>
    window.addEventListener("load", function (event) {
        // Adiciona o evento "blur" (mudança de foco) ao campo "Nombre"
        var nombreInput = document.getElementById("nombreField");
        nombreInput.addEventListener("blur", function (e) {
            validateSpecialChars(e.target);
        });
        // Adiciona o evento "blur" (mudança de foco) ao campo "Apellido"
        var apellidoInput = document.getElementById("apellidoField");
        apellidoInput.addEventListener("blur", function (e) {
            validateSpecialChars(e.target);
        });
        function validateSpecialChars(inputField) {
            // Verifica se há caracteres especiais no valor do campo
            var hasSpecialChars = /[~`!@#$%^&*()_+=[\]{}|\\:;"'<>,.?/]/.test(inputField.value);
            // Se houver caracteres especiais, exibe a mensagem de erro
            if (hasSpecialChars) {
                inputField.setCustomValidity("El campo no debe contener caracteres especiales.");
            } else {
                // Caso contrário, remove a mensagem de erro
                inputField.setCustomValidity("");
            }
        }
    });
</script>
